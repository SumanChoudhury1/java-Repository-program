
public class SumOFvalu {
   static int arr[]= {1,3,8,5,9,6,15,18,19};
	//int b=21;
  static int sum()
    {
	   int sum=21;
	   int i;
	   for( i=0;i<arr.length;i++) {
		   sum+=arr[i];
		   return sum;
	   }
   }
	public static void main(String[] args) {
      
    	  System.out.print("print sum of arry"+sum());
      }
	}

}
